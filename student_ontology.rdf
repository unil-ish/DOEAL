<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:ex="http://example.org/">
  <owl:Ontology rdf:about="http://example.org/"/>
  
  <owl:Class rdf:about="http://example.org/Person"/>
  <owl:Class rdf:about="http://example.org/Student">
    <rdfs:subClassOf rdf:resource="http://example.org/Person"/>
  </owl:Class>
  
  <owl:DatatypeProperty rdf:about="http://example.org/hasName"/>
  <owl:DatatypeProperty rdf:about="http://example.org/hasAge"/>
  <owl:ObjectProperty rdf:about="http://example.org/isEnrolledIn"/>
</rdf:RDF>
