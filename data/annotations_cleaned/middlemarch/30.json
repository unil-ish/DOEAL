   "feels": [
    {
     "name": "Satisfaction",
     "hasIntensity": 0.7,
     "quote": "Mr. Hawley was not slow to perceive that there was no handle for the law either in the revelations made by Raffles or in the circumstances of his death.",
     "causedBy": {
      "id": "texts/middlemarch.txt#No_legal_handle_in_Raffles_revelations",
